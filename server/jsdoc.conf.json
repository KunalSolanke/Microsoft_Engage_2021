{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "recurseDepth": 10,
  "source": {
    "include":["."],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|docs)"
  },
  "sourceType": "module",
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "Readme.md"
  }
}